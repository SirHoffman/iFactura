<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<servlet>
		<servlet-name>IFactura2016Servlet</servlet-name>
		<servlet-class>es.upm.dit.isst.iFactura2016.IFactura2016Servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>IFactura2016Servlet</servlet-name>
		<url-pattern>/ifactura2016</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>FacturasServlet</servlet-name>
		<servlet-class>es.upm.dit.isst.iFactura2016.FacturasServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FacturasServlet</servlet-name>
		<url-pattern>/facturas</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>NuevaFacturaServlet</servlet-name>
		<servlet-class>es.upm.dit.isst.iFactura2016.NuevaFacturaServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>NuevaFacturaServlet</servlet-name>
		<url-pattern>/nuevaFactura</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>NuevoUserServlet</servlet-name>
		<servlet-class>es.upm.dit.isst.iFactura2016.NuevoUserServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>NuevoUserServlet</servlet-name>
		<url-pattern>/nuevoUser</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>EditUserServlet</servlet-name>
		<servlet-class>es.upm.dit.isst.iFactura2016.EditUserServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EditUserServlet</servlet-name>
		<url-pattern>/editUser</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>FaqServlet</servlet-name>
		<servlet-class>es.upm.dit.isst.iFactura2016.FaqServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FaqServlet</servlet-name>
		<url-pattern>/faq</url-pattern>
	</servlet-mapping>
</web-app>
